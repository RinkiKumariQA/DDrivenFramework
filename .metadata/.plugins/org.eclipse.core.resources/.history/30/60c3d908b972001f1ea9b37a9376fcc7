package Interview_practice;

import java.util.HashSet;
import java.util.Scanner;
import java.util.Set;

public class javaa
{   
	public static void main(String[] args) 
{ 
     int temp=0;
     for(int i=1;i<=50;i++)
     {
    	 temp=0;
    	 for(int j=2;j<=i-1;j++)
    	 {
    		 if(i%j==0)
    		 {
    			 temp=temp+1;
    		 }
    		
    	 }
    	 if(temp==i)
 		{
 			System.out.println("prime number"+i);
 		}
     }
 
		
}
	
}
	
