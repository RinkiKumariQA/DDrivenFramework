package Inheritance;

import java.util.Scanner;

public class Fact {

	public static void main(String[] args) 
	{    
		 Scanner sc=new Scanner(System.in);
         System.out.println("Enthe the  value of n=");
         int n=sc.nextInt();
         
         for(int=f=1;n>=1;f=n*f;n--);
        	 
         {
        	 System.out.println(f);
         }
	}

}
