package Interview_practice;

import java.util.HashSet;
import java.util.Set;

public class javaa
{   
	public static void main(String[] args) 
	{ 
	  String s="Welcome Java";
      String ss=" ";
      char ch=' ';
      for(int i=0;i<s.length();i++)
      {
    	  if(ch!=s.charAt(i))
    	  {
    		  ss=ss+s.charAt(i);
    	  }
      }
      System.out.println(ss);

}}

