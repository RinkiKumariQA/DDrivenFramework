package Interview_practice;

import java.util.LinkedList;
import java.util.Queue;

public class javaa
{   
	public static void main(String[] args) 
	{
	   int count =0;
	   for(int i=1;i<=5;i++)
	   {
		   for(int j=1;j<=i;j++)
		   {
			   count =count+1;
			   System.out.print(count+ " ");
		   }
		   System.out.println();
	   }
	   
	   
	
	}
}