package Interview_practice;


public class javaa
{ 
   public static void main(String[] args)
   {
	
	  String s="Welcome landon";
	  String ss[]=s.split(s);
	  String rev="";
	  for(String s1:ss)
	  {
		  StringBuilder sb=new StringBuilder(s1);
		  sb.reverse();
		  rev=rev+sb.toString();
	  }

	  System.out.println(rev);
}
}	
