package Interview_practice;

import java.util.HashSet;
import java.util.Set;

public class javaa
{   
	public static void main(String[] args) 
	{ 
	  String s="Welcome Java";
	int count=1;
	for(int i=0;i<s.length();i++)
	{
		if(s.charAt(i)==' ')
		{
			count++;
		}
	}
	System.out.println(count);

}}

