package Interview_practice;

import java.util.HashSet;
import java.util.Set;

public class javaa
{   
	public static void main(String[] args) 
{ 
		
		int a=123;
		int pal=0;
		int temp;
		temp=a;
		while(a!=0)
		{
			int b=a%10;
			pal=pal*10+b;
			a=a%10;
		}
		
		if(a==temp)
		{
			System.out.println("palandrome");
		}
		else
		{
			System.out.println("not palindrome");
		}
}
}
	
