package Interview_practice;

import java.util.HashSet;
import java.util.Set;

public class javaa
{   
	public static void main(String[] args) 
	{ 
	  int a[]= {12,34,4,13,42,24,24,25,54,63,53};
	  int min=a[0];
	  for(int i=1;i<a.length;i++)
	  {
		  if(min>a[i])
		  {
			  min=a[i];
		  }
	  }

	  System.out.println(min);
}}

