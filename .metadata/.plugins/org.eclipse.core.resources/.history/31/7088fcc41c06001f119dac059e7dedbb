package Inheritance;

import java.util.Scanner;

public class ProgEvenreverse {

	public static void main(String[] args) 
	{
          Scanner sc=new Scanner(System.in);
          System.out.println("Enthe the  value of x=");
          int x=sc.nextInt();
          for(int i=1;i<=x;i++)
          {
        	  System.out.println(i*2);
          }
          
          
	}

}
