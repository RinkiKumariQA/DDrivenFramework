package Inheritance;

import java.util.Scanner;

public class Ques11 {

	public static void main(String[] args) // write  a program to first 10 even Natural number \\
	{
		for (int i=1;i<=10;i++)
		{
			System.out.println(i*2);
		}
       
		Scanner ss=new Scanner(System.in); // write  a program to first 10 even Natural number \\ Ques13
		{
		System.out.println("enter the value of a=");
		int a=ss.nextInt();
		for(int i=1;i<=a;i++)
		{
			System.out.println(i*2);
		}
		
		
		
	}

}
