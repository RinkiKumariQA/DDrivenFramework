package Inheritance;

import java.util.Scanner;

public class Reverse_number {

	public static void main(String[] args)
	{    int r=0;
	
		Scanner s1=new Scanner(System.in);
		System.out.println("enter the number");
		int y=s1.nextInt();
	
	
		  while(y>0)
		  {
		   int t=y%10;
			r=r*10+t;
			y=y%10;
		  }
          System.out.println(y);
	}

}
