package Inheritance;

import java.util.Scanner;

public class Reverse_number {

	public static void main(String[] args)
	{   
	
		Scanner s1=new Scanner(System.in);
		System.out.println("enter the number");
		int y=s1.nextInt();
		int z=s1.nextInt();
		
		  while(y>0)
		  {
		   z=y%10;
		   y=y%10;
		   
		   System.out.println("reverse numberof y");
		    
		  }
      
	}

}
