package testjava;

import java.util.HashMap;

public class HasMap {

	public static void main(String[] args) 
	{
	    HashMap m=new HashMap();
	    m.put(1,"Delhi");
	    m.put(2, "Ranchi");
	    m.put(3, "Noida");
	    m.put(4, "Pune");
	    System.out.println(m);
	    
	    HashMap mm=new HashMap();
	    mm.putAll(m);
	    System.out.println(mm); // putall method(copy constructor)
	    
	    HashMap m1=new HashMap();
	    m1.put(1,"Delhi");
	    m1.put(2, "Ranchi");
	    m1.put(3, "Noida");
	    m1.put(4, "Pune");
	    System.out.println(m1.get(3)); // object get key method
	    
	    HashMap m2=new HashMap();
	    m2.put(1,"Delhi");
	    m2.put(2, "Ranchi");
	    m2.put(3, "Noida");
	    m2.put(4, "Pune");
	    m2.remove(2);
	    System.out.println(m2); // remove object key method
	
	    HashMap m3=new HashMap();
	    m3.put(1,"Delhi");
	    m3.put(2, "Ranchi");
	    m3.put(3, "Noida");
	    m3.put(4, "Pune");
	    System.out.println(m3);
	    m3.replace(3, "Mumbai");
	   System.out.println(m3);  // object replaceb(key,value)
	   
	   HashMap m4=new HashMap();
	    m4.put(1,"Delhi");
	    m4.put(2, "Ranchi");
	    m4.put(3, "Noida");
	    m4.put(4, "Pune");
	   System.out.println(m4.containsKey(2)); // boolen contains ket(object key)
	   
	   HashMap m5=new HashMap();
	    m5.put(1,"Delhi");
	    m5.put(2, "Ranchi");
	    m5.put(3, "Noida");
	    m5.put(4, "Pune");
	    System.out.println(m5.containsValue("Pune")); //// boolen contains ket(object value
	    
	    HashMap m6=new HashMap();
	    m6.put(1,"Delhi");
	    m6.put(2, "Ranchi");
	    m6.put(3, "Noida");
	    m6.put(4, "Pune");
	    System.out.println(m6);
		   
	    HashMap m7=new HashMap();                      
	    m7.put(1,"Delhi");
	    m7.put(2, "Ranchi");
	    m7.put(3, "Noida");
	    m7.put(4, "Pune");
	    System.out.println(m7.size());  // in size method
	    
	    HashMap m8=new HashMap();
	    m8.put(1,"Delhi");
	    m8.put(2, "Ranchi");
	    m8.put(3, "Noida");
	    m8.put(4, "Pune");
	    System.out.println(m8.isEmpty()); // boolen is empty
	    
	    HashMap m9=new HashMap();
	    m9.put(1,"Delhi");
	    m9.put(2, "Ranchi");
	    m9.put(3, "Noida");
	    m9.put(4, "Pune");
	    System.out.println(m9.values()); // collection values( data display in the form of collection view
	    
	    HashMap m10=new HashMap();
	    m10.put(1,"Delhi");
	    m10.put(2, "Ranchi");
	    m10.put(3, "Noida");
	    m10.put(4, "Pune");
	    System.out.println(m10.keySet());// view containing all the keys in set mthod(key set)
	    
	    HashMap m11=new HashMap();
	    m11.put(1,"Delhi");            
	    m11.put(2, "Ranchi");
	    m11.put(3, "Noida");
	    m11.put(4, "Pune");
	    m11.put(5, "Raipur");
	    System.out.println(m11.entrySet());// view containing all the keys & values is set(enteryset)
	    
	    
	   
	    
           
	}

}
