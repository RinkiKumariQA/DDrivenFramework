package Practice;

public class Constructor {

	public static void main(String[] args) 
	{
        int a=6;
        a++;
        System.out.println(a);
        
        int b=4;
        --b;
      System.out.println(b);
      
      int x=4;
       int y=++x;
      System.out.println(x);
      System.out.println(y);
      
      int q=6;
      int r=--q;
      System.out.println(q);
      System.out.println(r);
    		  
	}

}
