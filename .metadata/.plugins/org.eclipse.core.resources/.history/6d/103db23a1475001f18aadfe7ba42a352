package Interview_practice;

public class Count {

	public static void main(String[] args) 
	{ 
		String s="Wow";
		String rev="";
		int l=s.length();
		String org_str=s;

		for(int i=l-1;i>=0;i--)
		{
			rev=rev+s.charAt(i);
		}
		 // System.out.println(rev);
		
		  if(org_str.equals(rev))
		  {
			  System.out.println("pal");
		  }
		  else
		  {
			  System.out.println("not pal ");
		  }
	}

}
