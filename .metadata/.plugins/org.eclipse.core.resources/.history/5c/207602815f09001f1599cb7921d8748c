package Arrayprogram;

public class Find_Duplicate_elements_array  // find duplicate elements in an array in java // Ques 3
{

	public static void main(String[] args)
	{
		int a[]= {12,24,12,87,99,90,22,22,28};
		for(int i=0;i<a.length;i++)
		{
			for(int j=i+1;j<a.length;j++)
			{
				if(a[i]==a[j])
			
			{
				System.out.println(a[i]);
			}
			}      
	}
		
		String ss="Hello Class";
		for(int j=0;j<ss.length();j++)
		{
			for(int k=j+1;k<ss.length();k++)
			{
              if(ss.charAt(j)==ss.charAt(k))
				{
					System.out.println(ss.charAt(j));
				}

	}
		}
	}
}
			
