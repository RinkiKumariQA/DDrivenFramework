package Interview_practice;

import java.util.HashSet;
import java.util.Set;

public class javaa
{   
	public static void main(String[] args) 
	{    
		
		
		 String s="Welcome";
		 String s1=" ";
		 int l=s.length();
		 for(int i=l-1;i>=0;i--)
		 {
		    s1=s1+s.charAt(i);
		 }
		   System.out.println(s1);
	}
	}
   
