package Inheritance;

import java.util.Scanner;

public class Reverse_number {

	public static void main(String[] args)
	{   
	
		Scanner s1=new Scanner(System.in);
		System.out.println("enter the number");
		int y=s1.nextInt();
		int rev=0;
		
		  while(y>0)
		  {
		   rev=y%10;
		   System.out.println(y);
		   y=y%10;
		   
		 
		    
		  }
      
	}

}
