package Interview_practice;


public class javaa
{ 
   public static void main(String[] args)
   {
	 
	   String str="My name is Rinki";
	   String s="";
	   String a[]=str.split(" ");
	   for(int i=0;i<str.length();i++)
	   {
	      System.out.print(a[i] +" ");
	   }
	   for(int i=a.length-1;i>=0;i--)
	   {
	     s=s+a[i]+" ";  
	   }
	     System.out.println(s);
   
   
}
}	
