package Inheritance;

import java.util.Scanner;

public class Progoddreverse {

	public static void main(String[] args) 
	{
       Scanner s1=new Scanner(System.in);
       System.out.println("enter the value of y=");
       int y=s1.nextInt();
       for(int i=2;i<=y;i++)
       {
    	   System.out.println(i*2-1);
       }
		
	}

}
